{
  "name": "test-travis",
  "version": "0.1.0",
  "engines": {
    "sketch": ">=3.0"
  },
  "skpm": {
    "name": "test-travis",
    "manifest": "src/manifest.json",
    "main": "plugin.sketchplugin"
